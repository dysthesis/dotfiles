[calendars]

[[private]]
path = ~/.local/share/khal/calendars/private
type = calendar

[[university]]
path = ~/.local/share/calendars/UNSW
color = yellow

[[personal]]
path = ~/.local/share/calendars/Personal
color = light blue

[[home]]
path = ~/.local/share/calendars/Home
color = light magenta

[[study]]
path = ~/.local/share/calendars/Study
color = light red

[locale]
timeformat = %H:%M
dateformat = %d/%m/%Y
longdateformat = %d/%m/%Y
datetimeformat = %d/%m/%Y %H:%M
longdatetimeformat = %d/%m/%Y %H:%M

[default]
default_calendar = private
